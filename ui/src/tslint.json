{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
          true,
          "attribute",
          "sb",
          "camelCase"
        ],
        "component-selector": [
          true,
          "element",
          "sb",
          "kebab-case"
        ],
      "whitespace": [
        true,
        "check-module"
      ],
      "quotemark": [
        true,
        "single"
      ]
    }
}
