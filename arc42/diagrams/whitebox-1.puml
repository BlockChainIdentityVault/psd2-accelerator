@startuml whitebox-1

skinparam shadowing false
scale 1200 width

actor "Bank Admin" as badmin
actor "TPP Developer" as tppd

package sandbox {

  [ssl-proxy] <<infrastructure>>

  [xs2a] --> [testdata]
  [config] <- badmin
  [xs2a] -> [config]

  () api -- [xs2a]
  tppd --> [ssl-proxy]
  [ssl-proxy] --> api
  tppd --> [developer-portal]
  tppd --> [certificate-generator]

}

@enduml
